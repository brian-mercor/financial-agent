{
  "name": "fin-agent2",
  "version": "1.0.0",
  "private": true,
  "description": "Hybrid financial AI system with Mastra framework",
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web --filter=backend",
    "dev:web-a": "turbo run dev --filter=web-a --filter=backend",
    "dev:web-b": "turbo run dev --filter=web-b --filter=backend",
    "dev:web-d": "turbo run dev --filter=web-d --filter=backend",
    "dev:web-e": "turbo run dev --filter=web-e --filter=backend",
    "dev:all": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "turbo": "^2.5.6",
    "typescript": "^5.7.3",
    "@types/node": "^20.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.3",
      "@types/react-dom": "18.3.0",
      "react": "18.3.1",
      "react-dom": "18.3.1"
    }
  }
}